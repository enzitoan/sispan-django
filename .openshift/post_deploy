source ${OPENSHIFT_HOMEDIR}python-3.3/virtenv/bin/activate

export PYTHON_EGG_CACHE=${OPENSHIFT_HOME_DIR}python-3.3/virtenv/lib/python-3.3/site-packages

echo "Executing 'python ${OPENSHIFT_REPO_DIR}sispan/manage.py syncdb --noinput'"
python "$OPENSHIFT_REPO_DIR"manage.py migrate --noinput

echo "Executing 'python ${OPENSHIFT_REPO_DIR}sispan/manage.py collectstatic --noinput -v0'"
python "$OPENSHIFT_REPO_DIR"manage.py collectstatic --noinput -v0
